{
  "name": "react-redux-saga-starter",
  "version": "0.0.1",
  "description": "Boilerplate for React Redux Saga applications",
  "main": "index.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --require ./test/setup.js --compilers js:babel-core/register \"src/**/*@(.test.js|.test.jsx)\"",
    "test:watch": "cross-env NODE_ENV=test mocha --watch --require ./test/setup.js --compilers js:babel-core/register \"src/**/*@(.test.js|.test.jsx)\"",
    "build:dev": "cross-env NODE_ENV=development webpack -d --progress --config ./webpack.development.config.js",
    "build:prod": "cross-env NODE_ENV=production webpack -p --progress --config ./webpack.production.config.js",
    "dev-server": "cross-env NODE_ENV=development webpack-dev-server --progress --config ./webpack.development.config.js"
  },
  "author": {
    "name": "Sebastian Prymas",
    "email": "prymas.sebastian@gmail.com"
  },
  "license": "MIT",
  "devDependencies": {
    "babel-core": "6.5.1",
    "babel-loader": "6.2.2",
    "babel-plugin-webpack-alias": "^2.1.2",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "6.5.0",
    "babel-preset-react": "6.5.0",
    "chai": "^3.5.0",
    "copy-webpack-plugin": "^4.0.1",
    "cross-env": "^3.2.4",
    "css-loader": "^0.26.2",
    "enzyme": "^2.7.1",
    "enzyme-redux": "^0.1.6",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "node-sass": "^3.8.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-diff-monitor": "^5.0.5",
    "redux-test-utils": "^0.1.2",
    "sass-loader": "^3.2.0",
    "sinon": "^2.1.0",
    "style-loader": "^0.13.0",
    "webpack": "1.12.14",
    "webpack-dev-server": "1.14.1"
  },
  "dependencies": {
    "immutable": "^3.8.1",
    "mirrorkey": "^1.2.0",
    "react": "^15.1.0",
    "react-dom": "^15.1.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.0.0",
    "redux": "^3.5.2",
    "redux-logger": "^2.8.2",
    "redux-saga": "^0.14.3"
  }
}
